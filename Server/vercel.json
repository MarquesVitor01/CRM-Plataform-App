{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/upload(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/generate-boleto-cnpj(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/generate-boleto-cpf(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/v1/charge(.*)",
        "dest": "/index.js"
      }
    ]
  }
  